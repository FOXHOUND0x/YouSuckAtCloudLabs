{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Action": [
              "aws-portal:ModifyAccount",
              "aws-portal:ModifyBilling",
              "aws-portal:ModifyPaymentMethods",
              "aws-portal:ViewBilling",
              "aws-portal:ViewBudget",
              "aws-portal:ViewPaymentMethods"
          ],
          "Resource": [
              "*"
          ],
          "Effect": "Deny",
          "Sid": "DisableBillingModification"
      },
      {
          "Action": [
              "cloudtrail:CreateTrail",
              "cloudtrail:DeleteTrail",
              "cloudtrail:StartLogging",
              "cloudtrail:StopLogging",
              "cloudtrail:UpdateTrail"
          ],
          "Resource": [
              "*"
          ],
          "Effect": "Deny",
          "Sid": "DisableCloudTrailModifications"
      },
      {
          "Action": [
              "iam:Attach*",
              "iam:Remove*",
              "iam:Update*",
              "iam:Deactivate*iam",
              "iam:Detach*"
          ],
          "Resource": [
              "arn:aws:iam::954093664884:policy/base-user-policies-*",
              "arn:aws:iam::954093664884:role/ReadOnly-SECURITY-RO",
              "arn:aws:iam::954093664884:role/Developer-SECURITY-RW",
              "arn:aws:iam::954093664884:role/Developer-SECURITY-RO",
              "arn:aws:iam::954093664884:role/Operations-SECURITY-RW",
              "arn:aws:iam::954093664884:role/Security-SECURITY-RW",
              "arn:aws:iam::954093664884:role/Networking-SECURITY-RW",
              "arn:aws:iam::954093664884:role/SRE-SECURITY-RW"
          ],
          "Effect": "Deny",
          "Sid": "DisableUserRoleModifications"
      },
      {
          "Condition": {
              "StringEquals": {
                  "ec2:ResourceTag/security": "*",
                  "ec2:ResourceTag/contact": "*",
                  "ec2:ResourceTag/department": "*",
                  "ec2:ResourceTag/service_id": "*",
                  "ec2:ResourceTag/application": "*",
                  "ec2:ResourceTag/applicationGroup": "*",
                  "ec2:ResourceTag/registry_id": "*"
              }
          },
          "Action": [
              "tag:addResourceTags",
              "tag:removeResourceTags"
          ],
          "Resource": [
              "*"
          ],
          "Effect": "Deny",
          "Sid": "DisableGovernanceTagModifications"
      },
      {
        "Action" :[
           "airflow:ListEnvironments",
            "airflow:GetEnvironment",
            "airflow:ListTagsForResource"
         ],
        "Effect": "Allow",
        "Resource":"*"
      },
      {
        "Action": "airflow:CreateWebLoginToken",
        "Effect": "Allow",
        "Resource":[
            "arn:aws:airflow:us-east-1:702226105286:role/fanstreams-prod-airflow/Viewer",
            "arn:aws:airflow:us-east-1:702226105286:role/brands-analytics-prod-airflow-mwaa-v3/Viewer",
            "arn:aws:airflow:us-east-1:702226105286:role/bi-prod-airflow-mwaa-v3/Viewer",
            "arn:aws:airflow:us-east-1:702226105286:role/fan-data-services-prod-airflow/Viewer"
          ]
      }
  ]
}