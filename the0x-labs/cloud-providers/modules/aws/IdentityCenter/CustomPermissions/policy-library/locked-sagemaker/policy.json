{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "SageMakerStudioAccess",
        "Effect": "Allow",
        "Action": [
          "logs:UpdateLogDelivery",
          "logs:PutResourcePolicy",
          "logs:PutLogEvents",
          "logs:ListLogDeliveries",
          "logs:GetLogEvents",
          "logs:GetLogDelivery",
          "logs:Describe*",
          "logs:DeleteLogDelivery",
          "logs:CreateLogStream",
          "logs:CreateLogGroup",
          "logs:CreateLogDelivery",
          "lambda:ListFunctions",
          "kms:ListAliases",
          "kms:DescribeKey",
          "iam:ListRoles",
          "groundtruthlabeling:*",
          "elasticfilesystem:DescribeMountTargets",
          "elasticfilesystem:DescribeFileSystems",
          "ecr:StartImageScan",
          "ecr:GetDownloadUrlForLayer",
          "ecr:GetAuthorizationToken",
          "ecr:Describe*",
          "ecr:CreateRepository",
          "ecr:BatchGetImage",
          "ecr:BatchCheckLayerAvailability",
          "ec2:DescribeVpcs",
          "ec2:DescribeVpcEndpoints",
          "ec2:DescribeSubnets",
          "ec2:DescribeSecurityGroups",
          "ec2:DescribeRouteTables",
          "ec2:DescribeNetworkInterfaces",
          "ec2:DescribeDhcpOptions",
          "cloudwatch:PutMetricData",
          "cloudwatch:PutMetricAlarm",
          "cloudwatch:ListMetrics",
          "cloudwatch:GetMetricStatistics",
          "cloudwatch:GetMetricData",
          "cloudwatch:DescribeAlarms",
          "cloudwatch:DeleteAlarms",
          "sagemaker:UpdateNotebookInstance",
          "sagemaker:UpdateAppImageConfig",
          "sagemaker:StopTransformJob",
          "sagemaker:StopTrainingJob",
          "sagemaker:StopProcessingJob",
          "sagemaker:StopNotebookInstance",
          "sagemaker:StopHyperParameterTuningJob",
          "sagemaker:StopEdgePackagingJob",
          "sagemaker:StartNotebookInstance",
          "sagemaker:ListUserProfiles",
          "sagemaker:ListTransformJobs",
          "sagemaker:ListTrainingJobsForHyperParameterTuningJob",
          "sagemaker:ListTrainingJobs",
          "sagemaker:ListSpaces",
          "sagemaker:ListProcessingJobs",
          "sagemaker:ListNotebookInstances",
          "sagemaker:ListModels",
          "sagemaker:ListHyperParameterTuningJobs",
          "sagemaker:ListExperiments",
          "sagemaker:ListEdgePackagingJobs",
          "sagemaker:ListDomains",
          "sagemaker:ListApps",
          "sagemaker:DescribeUserProfile",
          "sagemaker:DescribeTransformJob",
          "sagemaker:DescribeTrainingJob",
          "sagemaker:DescribeSpace",
          "sagemaker:DescribeProcessingJob",
          "sagemaker:DescribeNotebookInstance",
          "sagemaker:DescribeModel",
          "sagemaker:DescribeHyperParameterTuningJob",
          "sagemaker:DescribeExperiment",
          "sagemaker:DescribeEdgePackagingJob",
          "sagemaker:DescribeDomain",
          "sagemaker:DescribeApp",
          "sagemaker:DeleteNotebookInstance",
          "sagemaker:DeleteModel",
          "sagemaker:DeleteExperiment",
          "sagemaker:DeleteApp",
          "sagemaker:CreateTransformJob",
          "sagemaker:CreateTrainingJob",
          "sagemaker:CreateProcessingJob",
          "sagemaker:CreatePresignedDomainUrl",
          "sagemaker:CreateNotebookInstance",
          "sagemaker:CreateModel",
          "sagemaker:CreateHyperParameterTuningJob",
          "sagemaker:CreateExperiment",
          "sagemaker:CreateEdgePackagingJob",
          "sagemaker:CreateApp"
        ],
        "Resource": [
        "arn:aws:sagemaker:us-east-1:*:*/*",
        "arn:aws:ec2:us-east-1:*:*/*",
        "arn:aws:elasticfilesystem:us-east-1:*:*/*",
        "arn:aws:ecr:us-east-1:*:*/*",
        "arn:aws:logs:us-east-1:*:*/*"
      ]
      },
      {
        "Sid": "AllowSagemakerAccessForSpaces",
        "Effect": "Allow",
        "Action": [
          "tag:GetResources",
          "sagemaker:CreateSpace",
          "sagemaker:DeleteSpace",
          "sagemaker:UpdateSpace",
          "sagemaker:AddTags",
          "sagemaker:DescribeSpace"
        ],
        "Resource": [
          "*"
        ]
      },
      {
        "Sid": "AllowS3AccessToSpecificBucket",
        "Effect": "Allow",
        "Action": [
          "s3:List*",
          "s3:Get*",
          "s3:Put*"
        ],
        "Resource": [
          "arn:aws:s3:::ai-ml-mlflow",
          "arn:aws:s3:::ai-ml-mlflow/*"
        ]
      },
      {
        "Sid": "AllowEMRListAccess",
        "Effect": "Allow",
        "Action": [
          "elasticmapreduce:TerminateJobFlows",
          "elasticmapreduce:RunJobFlow",
          "elasticmapreduce:ModifyInstanceGroups",
          "elasticmapreduce:ListSteps",
          "elasticmapreduce:ListInstanceGroups",
          "elasticmapreduce:ListClusters",
          "elasticmapreduce:DescribeStep",
          "elasticmapreduce:DescribeCluster",
          "elasticmapreduce:CancelSteps",
          "elasticmapreduce:AddJobFlowSteps",
          "elasticmapreduce:ListInstances"
        ],
        "Resource": "*"
        }
    ]
}
      

     
  